{
  "source": {
    "include": ["./src","README.md"],
    "exclude": ["./docs","./dist", "./node_modules", "./coverage", "./test", "./index.js"],
    "includePattern": ".js$"
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "template": "./node_modules/minami"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "sort": false
  }
}
